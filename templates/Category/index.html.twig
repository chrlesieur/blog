{% extends 'base.html.twig' %}
{% block body %}
    {{ form_start(form) }}
    {{ form_row(form.name) }}
    <button class="btn">{{ button_label|default('Save') }}</button>
    {{ form_end(form) }}

    {% for category in categories %}
    <h1>{{ category.name}}</h1>
    {% endfor %}

{% endblock %}